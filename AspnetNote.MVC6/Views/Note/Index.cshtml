
<table class="table table-bordered">
    <thead>
        <tr>
            <th>No.</th>
            <th>Title</th>
            <th>Author</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var note in Model)
        {
            <tr>
                <td>@note.NoteNo</td>
                <td>
                    <a asp-controller="Note" asp-action="Detail" asp-route-NoteNo="@note.NoteNo">@note.NoteTitle</a>
                </td>
                <td>@note.UserNo</td>
            </tr>
        }
        
    </tbody>
</table>

<a class="btn btn-warning" asp-controller="Note" asp-action="Add">Add Content</a>